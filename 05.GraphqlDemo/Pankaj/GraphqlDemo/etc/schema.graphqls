type Query {
    getGreeting(name: String): String @resolver(class: "Pankaj\\GraphqlDemo\\Model\\Resolver\\Greeting")

    productBySku(sku: String!): ProductData
        @resolver(class: "Pankaj\\GraphqlDemo\\Model\\Resolver\\ProductBySku")
}

type ProductData {
    id: Int
    sku: String
    name: String
    price: Float
    description: String
}